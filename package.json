{
  "name": "@cognite/gearbox",
  "version": "1.11.0",
  "description": "GearBox will be a place for application developers to contribute useful, reusable components across applications",
  "contributors": [],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "cognite",
    "react",
    "components"
  ],
  "scripts": {
    "build": "yarn clean && rollup -c && yarn esCheck",
    "build:watch": "yarn clean && rollup -c --watch && yarn esCheck",
    "build-storybook": "build-storybook -s ./public",
    "clean": "rimraf dist",
    "esCheck": "es-check es5 ./dist/index.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "prettier \"src/**/*.ts\" --write && tslint -c tslint.json -p tsconfig.json --fix",
    "prepublishOnly": "yarn build",
    "test": "yarn jest",
    "test:watch": "yarn jest --watchAll",
    "storybook": "start-storybook -p 6006 -s ./public",
    "storybook-ssl": "start-storybook -p 6006 --https --ssl-cert ./server.cert --ssl-key ./server.key -s ./public"
  },
  "repository": "cognitedata/gearbox.js",
  "homepage": "https://github.com/cognitedata/gearbox.js#readme",
  "license": "Apache-2.0",
  "dependencies": {
    "d3": "^5.9.2",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.15",
    "ms": "^2.1.2",
    "numeral": "^2.0.6",
    "pinch-zoom-js": "^2.3.4",
    "react-dnd": "^7.4.5",
    "react-dnd-html5-backend": "^7.4.4",
    "react-odometerjs": "^2.1.1",
    "react-sizeme": "^2.5.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@cognite/griff-react": "~0.4.2",
    "@cognite/sdk": "^2.11.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/travis-cli": "^8.2.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@types/d3": "^5.7.1",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/fetch-mock": "^7.3.0",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^24.0.6",
    "@types/lodash": "^4.14.123",
    "@types/ms": "^0.7.31",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.9.2",
    "@types/react-test-renderer": "^16.8.1",
    "@types/sinon": "^7.0.9",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.13",
    "@types/uuid": "^3.4.4",
    "antd": "^3.14.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "es-check": "^5.0.0",
    "husky": "^3.0.5",
    "jest": "^24.8.0",
    "jest-css-modules-transform": "^2.1.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "react": "^16.10.2",
    "react-dnd-test-backend": "^7.4.4",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.8.5",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.26.5",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-typescript2": "^0.25.2",
    "sinon": "^7.2.7",
    "storybook-readme": "^5.0.8",
    "styled-components": "^4.1.3",
    "three": "^0.104.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.7.2",
    "utility-types": "^3.7.0"
  },
  "peerDependencies": {
    "@cognite/griff-react": "~0.4.2",
    "antd": "^3.14.1",
    "moment": "^2.24.0",
    "react": "~16.8",
    "styled-components": "^4.1.3"
  },
  "optionalDependencies": {
    "@cognite/3d-viewer": "^6.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier -- --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "npm": ">=4",
    "node": ">=6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  }
}
